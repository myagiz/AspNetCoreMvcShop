@model WebUI.Models.ProductListModel
@{
    ViewData["Title"] = "List";
    Layout = "~/Views/Shared/HomeLayout.cshtml";
}

<div class="row" style="font-family: Poppins">
    <div class="col-md-3">
        @await Component.InvokeAsync("CategoryList")
    </div>
    <div class="col-md-9">
        <div class="container">
            <div class="row">
                @foreach (var item in Model.Products)
                {
                    <partial name="_product" for="@item" />
                }
            </div>
            <div page-model="@Model.PageInfo">

            </div>
        </div>
    </div>
</div>



